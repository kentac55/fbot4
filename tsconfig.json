{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "ESNext.AsyncIterable"],
    "jsx": "react",
    "jsxFactory": "JSXSlack.h",
    "jsxFragmentFactory": "JSXSlack.Fragment"
  },
  "include": ["src/*.ts", "src/**/*.ts", "test/*.ts", "test/**/*.ts"],
  "exclude": ["src/**/*.test.ts"]
}
